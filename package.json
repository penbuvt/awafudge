  },
  "homepage": "https://github.com/penbuvt/awafudge#readme",
  "devDependencies": {
    "@ts-jison/parser-generator": "^0.4.1-alpha.2",
    "@types/mocha": "^10.0.7",
    "mocha": "^10.2.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4"
  },
  "dependencies": {
    "nopt": "^8.1.0"
  }
